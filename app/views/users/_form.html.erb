<%= simple_form_for @user do |f| %>
  <%= f.input :first_name %>
  <%= f.input :last_name %>
  <%= f.input :email %>
  <%= f.input :phone_number %>
  <%= f.input :city %>
  <%= f.input :state %>
  <%= f.input :date_of_birth %>
  <%= f.input :password %>
  <%= f.input :password_confirmation %>
  <%= f.input :about_me %>
  <%= f.button :submit %>
<% end %>
